import java.util.*;
class Solution{
   public static void main(String args[]){
      Scanner scanner = new Scanner(System.in);
      System.out.println("Enter the number:");
      int num = scanner.nextInt();
      int k = fact(num);
      System.out.println("Factorial is: "+k);
   }
   static int fact(int n)
   {
       int output;
       if(n==1){
         return 1;
       }
       output = fact(n-1)* n;
       return output;
   }
}
